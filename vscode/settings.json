{
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.fontSize": 12,
    "editor.fontFamily": "Fira Code Retina, monospace",
    "editor.fontLigatures": true,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.minimap.showSlider": "always",
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80
    ],
    "editor.scrollBeyondLastColumn": 2,
    "editor.semanticHighlighting.enabled": true,
    "editor.snippetSuggestions": "top",
    "elixirLS.dialyzerWarnOpts": [
        "unmatched_returns",
        "error_handling"
    ],
    "evenBetterToml.formatter.arrayAutoExpand": true,
    "evenBetterToml.formatter.arrayTrailingComma": true,
    "evenBetterToml.formatter.reorderArrays": true,
    "explorer.confirmDragAndDrop": false,
    "extensions.autoUpdate": false,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "erlinit.config": "shellscript",
        "sxhkdrc": "properties",
        "vimrc": "viml",
        "vm.args": "shellscript",
        "vm.args.eex": "shellscript",
        "zshrc": "shellscript",
        ".clang-format": "yaml",
        ".envrc": "shellscript",
        ".gitignore.eex": "gitignore",
        "*.ex.eex": "elixir",
        "*.exs.eex": "elixir",
        "*.md.eex": "markdown",
        "*.nix.eex": "nix",
        "*.toml.eex": "toml"
    },
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git-graph.dialog.rebase.ignoreDate": false,
    "git-graph.repository.fetchAndPrune": true,
    "gitlens.codeLens.scopes": [
        "document",
        "containers",
        "blocks"
    ],
    "gitlens.currentLine.enabled": false,
    "gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss Y",
    "gitlens.defaultDateShortFormat": "YYYY-MM-DD",
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.pullRequests.enabled": false,
    "gitlens.plusFeatures.enabled": false,
    "gitlens.statusBar.alignment": "left",
    "gitlens.statusBar.pullRequests.enabled": false,
    "gitlens.views.branches.branches.layout": "list",
    "gitlens.views.branches.pullRequests.enabled": false,
    "gitlens.views.commits.pullRequests.enabled": false,
    "gitlens.views.contributors.pullRequests.enabled": false,
    "gitlens.views.repositories.branches.layout": "list",
    "gitlens.views.repositories.pullRequests.enabled": false,
    "gitlens.views.remotes.branches.layout": "list",
    "gitlens.views.remotes.pullRequests.enabled": false,
    "latex-workshop.chktex.args.active": [
        "-wall",
        "-n1",
        "-n13",
        "-n19",
        "-n22",
        "-n30",
        "-n44",
        "-e16",
        "-q"
    ],
    "latex-workshop.chktex.args.root": [
        "-wall",
        "-n1",
        "-n13",
        "-n19",
        "-n22",
        "-n30",
        "-n44",
        "-e16",
        "-q"
    ],
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.aux.bak",
        "*.bbl",
        "*.bcf",
        "*.blg",
        "*.brf",
        "*.dvi",
        "*.fdb_latexmk",
        "*.fls",
        "*.glg",
        "*.glo",
